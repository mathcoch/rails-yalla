<% if notice %>
<script>
notif({
  msg: "<%= notice %>",
  type: "success",
  timeout: 5000,
  position: "right",
  opacity: 0.7
});
</script>
<% end %>
<% if alert %>
<script>
notif({
  msg: "<%= alert %>",
  type: "error",
  timeout: 3000,
  position: "right",
  opacity: 0.7
});
</script>
<% end %>
